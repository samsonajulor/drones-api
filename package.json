{
  "name": "core-engine",
  "version": "1.0.0",
  "description": "dispatch controller",
  "main": "index.js",
  "author": "Samson Ajulor",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=html mocha src/test/*.js --timeout 10000 -c --require @babel/register --exit",
    "migrate": "sequelize db:migrate ",
    "migrate:reset": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "migrate:seed": "npm run migrate && npm run seed",
    "watch-ts": "tsc -w",
    "start": "node dist/index.js",
    "dev": "cross-env nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "build": "cross-env rimraf ./dist && tsc",
    "build-ts": "tsc",
    "format": "cross-env prettier --write ./**/*.{js,ts,md,json} --config ./.prettierrc"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@types/hapi__joi": "15.0.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "cross-env": "^7.0.3",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.17.1",
    "http-status-codes": "2.2.0",
    "joi": "17.6.0",
    "js2xmlparser": "5.0.0",
    "moment": "^2.29.3",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.15",
    "pg": "8.5.1",
    "pg-hstore": "2.3.3",
    "sequelize": "5.8.5",
    "sequelize-cli": "5.4.0",
    "ts-node": "^10.9.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/node": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.8",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.12.17",
    "@babel/register": "^7.5.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.11",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^12.7.8",
    "@types/pg": "7.14.10",
    "@types/sequelize": "4.28.14",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.3.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.8.4"
  }
}
